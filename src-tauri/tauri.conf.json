{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Koszos Quiz",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "readFile": true,
        "scope": ["RESOURCE/*"]
      },
      "http": {
        "all": true
      },
      "window": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "koszos_quiz",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "decorations": true,
        "center": true,
        "fullscreen": false,
        "resizable": true,
        "title": "Koszos Quiz",
        "width": 1280,
        "height": 720,
        "minWidth": 800,
        "minHeight": 720,
        "titleBarStyle": "Overlay"
      }
    ]
  }
}
